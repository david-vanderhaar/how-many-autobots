{
    "name": "how-many-autobots",
    "description": "How MAny Autobots?",
    "version": "0.0.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phaserjs/template-vite.git"
    },
    "author": "David Vanderhaar",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "scripts": {
        "start": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "start-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "deploy": "pnpm run build && gh-pages -d dist"
    },
    "devDependencies": {
        "terser": "^5.39.0",
        "vite": "^6.3.1"
    },
    "dependencies": {
        "gh-pages": "^6.3.0",
        "phaser": "^3.90.0"
    }
}